<%= stylesheet_link_tag "application" %>
<script>
  document.addEventListener("DOMContentLoaded", function () {
    var popup = document.getElementById("popup");
    popup.style.display = "none";
    var routeOptions = document.querySelectorAll(".route-option"); // Select all elements with the class "route-option"
    routeOptions.forEach(function (element) {
      element.style.cursor = "pointer";

      element.onclick = function () {
        popup.style.display = "flex";
        var parameter = element.getAttribute("data-parameter");
        var pokemonName = document.getElementById("pokemon-name");
        pokemonName.innerHTML = "Pokemon: " + parameter;
      };

      element.addEventListener("mouseover", function () {
        var parameter = element.getAttribute("data-parameter");
        var placeName = document.getElementById("place-name");
        placeName.innerHTML = parameter;
      });
    });
  });
</script>
<div class="popup-wrapper" id="popup"><%= render 'popup' %></div>
<div class="info-wrapper">
  <div class="info" id="place-name">Place</div>
</div>
